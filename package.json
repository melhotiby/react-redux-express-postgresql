{
  "name": "woodlogic",
  "version": "1.0.0",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "server": "nodemon ./server/index.js",
    "client": "cd client && yarn start",
    "db:make": "knex migrate:make",
    "db:create": "node ./server/db/create.js",
    "db:seed": "knex seed:run",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "webhook": "./sendgrid_webhook.sh",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "cd client && NPM_CONFIG_PRODUCTION=false yarn install && yarn build"
  },
  "engines": {
    "node": "12.x"
  },
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "concurrently": "^5.2.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "http-proxy-middleware": "^1.0.5",
    "knex": "^0.21.2",
    "localtunnel": "^2.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "path-parser": "^6.1.0",
    "pg": "^8.3.0",
    "ramda": "^0.27.0",
    "sendgrid": "^5.2.3"
  },
  "devDependencies": {
    "faker": "^4.1.0"
  }
}
